Tab=[]
#choice1_ft
def addTab(Title,Url):
    Tab.append({})
    Tab[len(Tab)-1][Title]=Url
    return Tab
print('''
1. Open Tab
2. Close Tab
3. Switch Tab
4. Display All Tabs
5. Open Nested Tab
6. Clear All Tabs
7. Save Tabs
8. Import Tabs
9. Exit''')
choice=''
while choice !='9':
    choice=input('Enter your choice : ')
    while not choice.isnumeric():
        print('please enter an integer from 1 to 9 :')
